<p> </p>
<p><%= link_to "Repeat", dashboard_path%> </p>
<h3>Results</h3>
<table>
  <th>
    <td>Amount right symbols</td>
    <td>Correct input percent</td>
    <td>Time</td>
    <td>Symbols per minute</td>
  </th>
  <tr>
    <td><%= @k %></td>
    <% if @k != 0 then%>
      <td><%= @text_game.length / @k %></td>
    <%else%>
      <td><%= @k %></td>
    <%end%>
    <td><%= @del_time.round(2)%></td>
    <td><%= @wpm%></td>
  </tr>
</table>
